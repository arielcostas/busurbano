{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "urbanovigo-web": {
      "appLocation": ".",
      "apiLocation": "Backend",
      "outputLocation": "dist",
      "apiLanguage": "dotnetisolated",
      "apiVersion": "8.0",
      "appBuildCommand": "yarn build",
      "apiBuildCommand": "dotnet publish -c Release",
      "run": "yarn dev",
      "appDevserverUrl": "http://localhost:5173"
    }
  }
}